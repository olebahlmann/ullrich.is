{
  "name": "ullrich.is",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.1.1",
    "next": "4.0.1",
    "nextein": "1.0.0-beta25",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "scripts": {
    "dev": "nextein",
    "export": "nextein build && nextein export",
    "deploy": "npm run export && now out --public",
    "analyze": "cross-env ANALYZE=1 nextein build",
    "postinstall": "npm run flow-typed-install",
    "flow-typed-install": "flow-typed install -s -o -i dev bundle peer",
    "flow-typed-createstub": "flow-typed create-stub -o"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.9.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.57.2",
    "flow-typed": "^2.2.0",
    "now": "^8.3.10"
  }
}
